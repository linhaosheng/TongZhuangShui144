<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/set_bg_btn21">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="领入押金本"
            android:textColor="#000000"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/dp_17"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_12"/>

        <ImageView
            android:id="@+id/delete"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_40"
            android:src="@mipmap/delete"
            android:layout_alignParentRight="true"
            android:scaleType="center"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_16"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/deposit_num_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="押金本编号"
            android:textColor="#333333"
            android:textSize="14sp"
            android:layout_marginLeft="@dimen/dp_24"
            android:layout_marginTop="@dimen/dp_14"/>

        <EditText
            android:id="@+id/deposit_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_50"
            android:layout_toRightOf="@+id/deposit_num_txt"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_14"
            android:background="@null"
            android:textSize="14sp"
            android:textColor="#333333"
            android:hint="请输入押金本编号"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#E6E6E6"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_below="@+id/deposit_num_txt"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/start_deposit_num_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="开始编号"
            android:textColor="#333333"
            android:textSize="14sp"
            android:textStyle="bold"
            android:layout_marginLeft="@dimen/dp_24"
            android:layout_marginTop="@dimen/dp_30"/>

        <EditText
            android:id="@+id/start_deposit_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_50"
            android:layout_toRightOf="@+id/start_deposit_num_txt"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_30"
            android:background="@null"
            android:textSize="14sp"
            android:textColor="#333333"
            android:hint="请输入开始编号"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#E6E6E6"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_below="@+id/start_deposit_num_txt"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/end_deposit_num_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="结束编号"
            android:textColor="#333333"
            android:textSize="14sp"
            android:textStyle="bold"
            android:layout_marginLeft="@dimen/dp_24"
            android:layout_marginTop="@dimen/dp_30"/>

        <EditText
            android:id="@+id/end_deposit_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_50"
            android:layout_toRightOf="@+id/end_deposit_num_txt"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_30"
            android:background="@null"
            android:textSize="14sp"
            android:textColor="#333333"
            android:hint="请输入结束编号"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#E6E6E6"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_below="@+id/end_deposit_num_txt"/>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_57"
        android:layout_marginTop="@dimen/dp_37">

        <Button
            android:id="@+id/cancel_btn"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="取消"
            android:textColor="#333333"
            android:background="#fff"
            android:textSize="16sp"/>

        <Button
            android:id="@+id/input_btn"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="提交"
            android:textSize="16sp"
            android:textColor="#fff"
            android:background="#32C5FF"
            />
    </LinearLayout>


</LinearLayout>