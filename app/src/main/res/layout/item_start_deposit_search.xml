<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_10"
    android:layout_marginTop="@dimen/dp_10"
    android:layout_marginRight="@dimen/dp_10"
    android:paddingBottom="@dimen/dp_15"
    android:background="@drawable/set_bg_btn13">

    <ImageView
        android:id="@+id/order_state"
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        android:layout_alignParentRight="true"
        android:src="@mipmap/void_state" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/deposit_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="13dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="押金编号：200003131"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/start_code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="押金本编号：32132432423424"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/end_code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="客户名称：佳佳超市"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/code_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="开押物品：空桶"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/with_drawal_price"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="开押单价：20"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/with_drawal_num"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="数量 ：5"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/with_drawal_money"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="金额 ：150"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />


        <TextView
            android:id="@+id/deposit_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="开押类型 ：押金"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/start_drawal_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="开押时间 ：2020-11-23"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/start_drawal_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="开押人 ：张三"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/with_drawal_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="退押时间 ：2020-11-23"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/with_drawal_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_32"
            android:layout_marginTop="15dp"
            android:layout_marginRight="@dimen/dp_30"
            android:text="退押人 ：张三"
            android:textColor="#3F3F3F"
            android:textSize="12sp" />


    </LinearLayout>

</RelativeLayout>